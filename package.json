{
  "name": "admin-manage",
  "version": "3.7.0",
  "description": "admin manage services",
  "private": true,
  "license": "ISC",
  "scripts": {
    "postinstall": "",
    "presite": "cd functions && npm install",
    "serve": "yarn start",
    "start": "cross-env TAILWIND_MODE=watch MOCK=none PORT=18888 max dev",
    "start:no-mock": "cross-env MOCK=none PORT=8888 max dev",
    "start:mock": "cross-env APP_TYPE=site PORT=8888 max dev",
    "build": "npx max build",
    "build:prod": "cross-env TYPE=prod && max build",
    "site": "npm run presite && cross-env APP_TYPE=site npm run build && firebase deploy && npm run docker:push",
    "analyze:dev": "cross-env ANALYZE=1 yarn start",
    "analyze:prod": "cross-env ANALYZE=1 max build",
    "speed:dev": "cross-env SPEED_MEASURE=JSON yarn start",
    "speed:prod": "cross-env SPEED_MEASURE=JSON yarn build",
    "lint:js": "eslint --ext .js src mock tests",
    "lint:style": "stylelint 'src/**/*.less' --syntax less",
    "lint:prettier": "check-prettier lint",
    "lint": "eslint --ext .js src mock tests && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --ext .js src mock tests && stylelint --fix 'src/**/*.less' --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "tslint": "npm run tslint:fix",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'",
    "test": "max test",
    "test:component": "max test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker-prod:dev": "docker-compose -f ./docker/docker-compose.yml up",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  ant-design-pro ./",
    "docker:tag": "docker tag ant-design-pro antdesign/ant-design-pro",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push antdesign/ant-design-pro"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.1",
    "@ant-design/compatible": "^1.0.2",
    "@ant-design/icons": "^4.0.6",
    "@antv/data-set": "^0.10.1",
    "@babel/runtime": "^7.9.6",
    "@typescript-eslint/parser": "^5.51.0",
    "@umijs/max": "^4.0.65",
    "ahooks": "^3.7.4",
    "ali-oss": "^6.8.0",
    "alife-logger": "^1.8.30",
    "antd": "4.20.0",
    "axios": "^0.19.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "bizcharts": "^4.1.22",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.6",
    "cookies-js": "^1.2.3",
    "copy-to-clipboard": "^3.3.1",
    "dva": "^2.4.1",
    "enquire-js": "^0.2.1",
    "hash.js": "^1.1.7",
    "html2canvas": "^1.0.0-rc.5",
    "immutability-helper": "^3.1.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.1",
    "kx-des": "^0.1.4",
    "kx-validator": "^0.1.8",
    "lodash-decorators": "^6.0.1",
    "lodash-es": "^4.17.21",
    "memoize-one": "^5.0.0",
    "moment": "2.29.4",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.0",
    "oss-operate": "^0.2.6",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "qs": "^6.11.1",
    "query-string": "4.1.0",
    "rc-animate": "^2.6.0",
    "react": "^18.2.0",
    "react-amap": "^1.2.8",
    "react-amap-plugin-geolocation": "^0.0.2",
    "react-color": "^2.19.3",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.2.0",
    "react-fittext": "^1.0.0",
    "react-media": "^1.9.2",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.26.1",
    "reactcss": "^1.2.3",
    "redux-actions": "^2.6.5",
